        <test-artifact>
            <artifact>/UnittestSampleProject/src/main/synapse-config/sequences/firstSubFlow.xml</artifact>
        </test-artifact>
        <supportive-artifacts/>
        <registry-resources/>
        <connector-resources/>
    </artifacts>
    <test-cases>
        <test-case name="firstSubFlow">
            <input>
                <properties>
                    <property name="flowValue" value="flowValue_1"/>
