            </input>
            <assertions>
                <assertEquals>
                    <actual>$ctx:flowValue</actual>
                    <expected><![CDATA[flowValue_1]]></expected>
                    <message>flowValue property value does not equal to the expected value</message>
                </assertEquals>
            </assertions>
        </test-case>
    </test-cases>
    <mock-services/>
</unit-test>
