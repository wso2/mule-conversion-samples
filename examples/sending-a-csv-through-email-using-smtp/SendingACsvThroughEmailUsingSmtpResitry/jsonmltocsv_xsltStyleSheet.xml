<?xml version="1.0" encoding="UTF-8" standalone="no"?><xsl:stylesheet xmlns:xsl="http://www.w3.org/1999/XSL/Transform" xmlns:firstElementOfTheInput="root" xmlns:notXSLTCompatible="false" xmlns:own="http://ownfunction.org" xmlns:xs="http://www.w3.org/2001/XMLSchema" extension-element-prefixes="xs own notXSLTCompatible firstElementOfTheInput" version="2.0"><xsl:function name="own:setPrecision"><xsl:param name="resultString"/><xsl:param name="noOfDigits"/><xsl:if test="$noOfDigits=0"><xsl:value-of select="$resultString"/></xsl:if><xsl:if test="$noOfDigits!=0"><xsl:value-of select="own:setPrecision(concat($resultString,'0'),$noOfDigits - 1)"/></xsl:if></xsl:function><xsl:template match="/"><xsl:for-each select="root"><root><name><xsl:value-of select="name"/></name><orderId><xsl:value-of select="orderId"/></orderId><pricePerUnit><xsl:value-of select="number( pricePerUnit )"/></pricePerUnit><units><xsl:value-of select="number( units )"/></units><totalPrice><xsl:value-of select="( number( units ) * number( pricePerUnit ) )"/></totalPrice></root></xsl:for-each></xsl:template></xsl:stylesheet>